{
  "ThirdPartyPriceCheckURL": "https://finnhub.io/api/v1/quote?symbol=[Ticker]&token=[token]",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  //"profiles": {
  //  "https": {
  //    "commandName": "Project",
  //    "launchBrowser": true,
  //    "launchUrl": "swagger/index.html",
  //    "applicationUrl": "https://localhost:7250",
  //    "environmentVariables": {
  //      "ASPNETCORE_ENVIRONMENT": "Development"
  //    }
  //  }
  //},
  "Kestrel": {
    "Endpoints": {
      "RestApi": {
        "Url": "https://localhost:7250",
        "Protocols": "Http1"
      },
      "GrpcApi": {
        "Url": "https://localhost:7001",
        "Protocols": "Http2"
      }
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs\\log.txt",
          "formatter": {
            "type": "Serilog.Templates.ExpressionTemplate, Serilog.Expressions",
            "template": "{@t:yyyy-MM-dd HH:mm:ss.fff}|{@l:u3}|{Substring(SourceContext, LastIndexOf(SourceContext,'.')+1)}|{@m:lj}\n{@x}"
          },
          "rollOnFileSizeLimit": "true",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 1
        },
        "Filter": [
          {
            "Name": "ByExcluding",
            "Args": {
              "expression": "Substring(SourceContext, LastIndexOf(SourceContext,'.')+1) = 'Diagnostics') or Substring(SourceContext, LastIndexOf(SourceContext,'.')+1)='Lifetime') or EndsWith(Substring(SourceContext, LastIndexOf(SourceContext,'.')+1), 'Middleware')"
            }
          }
        ]
      }
    ]
  }
}
